<?xml version="1.0" encoding="UTF-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPremiumWhite"
    android:orientation="vertical"
    tools:context="com.shamrock.reework.activity.InterpretationModule.activity.InterpretationActivity">

    <include
        android:id="@+id/custom_toolbar"
        layout="@layout/custom_toolbar" />

    <!--    <include-->

    <!--        layout="@layout/content_interpretation" />-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginRight="15dp"
        android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"

                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:background="#F3F6F8"
                android:padding="@dimen/_10sdp">



                <RelativeLayout
                    android:id="@+id/r1"
                    android:layout_below="@+id/linear_logout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.hookedonplay.decoviewlib.DecoView
                        android:layout_width="match_parent"
                        android:layout_height="130dp"
                        android:visibility="visible"
                        android:id="@+id/dynamicArcView"/>

                    <com.mikhaellopez.circularprogressbar.CircularProgressBar
                        android:layout_width="match_parent"
                        android:id="@+id/progress_circular_consumed"
                        android:layout_height="150dp"
                        android:layout_alignParentTop="true"
                        android:layout_margin="@dimen/_5sdp"
                        android:layout_centerInParent="true"
                        android:visibility="gone"
                        app:cpb_progressbar_color="#58C960"
                        app:cpb_progressbar_width="3dp" />
                    <TextView

                        android:id="@+id/txt_user_newScore"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="0dp"
                        android:layout_marginStart="10dp"
                        android:gravity="center"
                        android:visibility="visible"
                        android:autoSizeMaxTextSize="20dp"
                        android:layout_below="@+id/healthnew"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="0dp"
                        android:text="95"
                        android:textColor="#231F20"
                        android:textStyle="bold"
                        android:textSize="33dp"
                        />

                    <LinearLayout
                        android:id="@+id/healthnew"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginTop="17dp"

                        android:orientation="horizontal">
                        <ImageView

                            android:src="@drawable/healthnew"
                            android:layout_width="60dp"

                            android:layout_height="60dp"/>

                    </LinearLayout>



                </RelativeLayout>


                <TextView

                    android:id="@+id/txt_viewsteps12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="0dp"
                    android:layout_marginStart="10dp"
                    android:gravity="center"
                    android:visibility="visible"
                    android:autoSizeMaxTextSize="20dp"
                    android:layout_below="@+id/r1"
                    android:layout_centerInParent="true"
                    android:layout_marginTop="10dp"
                    android:text="Enjoy Good Health"
                    android:textColor="#231F20"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    />
                <TextView

                    android:id="@+id/txt_rescore_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="0dp"
                    android:layout_marginStart="10dp"
                    android:gravity="center"
                    android:visibility="visible"
                    android:autoSizeMaxTextSize="20dp"
                    android:layout_below="@+id/txt_viewsteps12"
                    android:layout_centerInParent="true"
                    android:layout_marginTop="0dp"
                    android:text="Anoop Kumar"
                    android:textColor="#00D5BC"
                    android:textStyle="bold"
                    android:textSize="32sp"
                    />

                <TextView

                    android:id="@+id/txt_viewsteps1234"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="0dp"
                    android:layout_marginStart="10dp"
                    android:gravity="center"
                    android:visibility="visible"
                    android:autoSizeMaxTextSize="20dp"
                    android:layout_below="@+id/txt_rescore_name"
                    android:layout_centerInParent="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="10dp"
                    android:text="Health is Wealth"
                    android:textColor="#7B7979"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    />

            </RelativeLayout>
</LinearLayout>


    <ScrollView
        android:scrollbars="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="15dp"
            android:orientation="vertical">



            <RelativeLayout
                android:layout_width="match_parent"

                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:background="#F3F6F8"
                android:padding="@dimen/_10sdp">

                <TextView

                    android:id="@+id/txt_viewste12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="0dp"
                    android:layout_marginStart="10dp"
                    android:gravity="start"
                    android:visibility="visible"



                    android:layout_marginTop="0dp"
                    android:text="Health Summary  "
                    android:textColor="#231F20"
                    android:textStyle="bold"
                    android:textSize="14dp"
                    />

                <TextView

                    android:id="@+id/txt_health_summery"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="0dp"
                    android:layout_marginStart="10dp"
                    android:gravity="start"
                    android:visibility="visible"


                    android:layout_marginTop="0dp"
                    android:text="At first, I would like to congratulate you for showing consciousness towards your health  enrolling in REEwork. â€œThe lockdown has served the purpose of putting a check on coronavirus"
                    android:layout_below="@+id/txt_viewste12"
                    android:textColor="#263238"
                    android:textStyle="bold"
                    android:textSize="12dp"
                    />

            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/lay_view"
                android:layout_width="match_parent"
                android:visibility="gone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginEnd="@dimen/_5sdp"

                >





            </RelativeLayout>

            <!--            <LinearLayout-->
            <!--                android:layout_width="match_parent"-->
            <!--                android:layout_height="wrap_content"-->

            <!--                android:layout_marginTop="@dimen/_5sdp"-->

            <!--                android:orientation="vertical">-->

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyler_wellness"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="visible"
                android:scrollbars="vertical"
                android:layout_marginTop="-20dp"
                />

            <!--            </LinearLayout>-->


            <Button
                android:id="@+id/buttonSubmit_MyProfiless"
                style="@style/button_Style_cal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:text="Next"
                android:textStyle="bold"
                android:visibility="visible" />


        </LinearLayout>

    </ScrollView>


