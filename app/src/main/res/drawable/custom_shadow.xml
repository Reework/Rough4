<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="360"
                android:endColor="@color/colorGreen5"
                android:startColor="@color/colorBlueGreen5"
                />
            <corners android:radius="6dp" />

        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape>
            <corners android:radius="5dp" />
            <solid
                android:endColor="@color/colorGreen5"
                android:startColor="@color/colorBlueGreen5"
                />
        </shape>
    </item>
</layer-list>

